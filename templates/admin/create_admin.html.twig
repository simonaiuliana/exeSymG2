{% extends 'base.html.twig' %}

{% block title %}Create Admin{% endblock %}

{% block body %}
<h1>Create Admin</h1>

{{ form_start(form) }}
    {{ form_row(form.username) }}
    {{ form_row(form.email) }}
    {{ form_row(form.password) }}
    <button type="submit" class="btn btn-primary">Create</button>
{{ form_end(form) }}

{% endblock %}